TMP=$(pwd)
cd /root/src
stop-http
set -e
start-http

for file in /root/src/**/*-test.cc
do
  filename="${file%.*}"
  echo "building ${filename}"
  rel=$(realpath --relative-to="/root/src" "$filename")
  ib $rel
done

for file in /root/src/**/*-test.cc
do
  filename="${file%.*}"
  rel=$(realpath --relative-to="/root/src" "$filename")
  ../out/debug/$rel
done

stop-http

cd $TMP
